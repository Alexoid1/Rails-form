<h1>There are <%= @users.count%> Users Stored</h1>
<ul>
    <% @users.each do |user| %>
        <li><%= user.username%></li>
        <li><%= user.email%></li>
        <li><a href="/users/<%= user.id %>">Show</a></li>
        <li><a href="/users/<%= user.id %>/edit">Edit</a></li>
        <li><a href="/users/<%= user.id %>" data-method="delete" data-confirm="Are you sure?">Delete</a></li>
        
        <hr>
    <% end %>
</ul>
<%= link_to "<< Back to User new", new_user_path %>